{
	"name": "kronos/encrypt",
	"description": "Library to help text encryption/decryption",
	"type": "library",
	"autoload": {
		"psr-4": {
			"Kronos\\Encrypt\\": "src/Kronos/Encrypt"
		}
	},
	"config": {
		"platform": {
			"php": "7.4"
		},
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": false
		}
	},
	"require": {
        "php": "^7.4 || ^8.1",
		"aws/aws-sdk-php": "3.224.3",
		"phpseclib/phpseclib": "2.0.37"
	},
	"require-dev": {
		"phpunit/phpunit": "9.5.20",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
		"phpcompatibility/php-compatibility": "^9.3",
		"vimeo/psalm": "4.23.0"
	},
	"license": "MIT"
}
